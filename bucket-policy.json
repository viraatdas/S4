{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowAmplifyAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "amplify.amazonaws.com"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::s4-frontend-deployment",
        "arn:aws:s3:::s4-frontend-deployment/*"
      ]
    }
  ]
}
